{
  "expo": {
    "name": "Henry Portfolio",
    "slug": "HenryPortfolio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/the_robot.png",
    "splash": {
      "image": "./assets/the_robot.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Small project for mobile and web app to display my introduction using React Native with TypeScript on Expo CLI",
    "githubUrl": "https://github.com/PlayerProgramming/HenryPortfolio"
  }
}
